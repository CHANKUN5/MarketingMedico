{
  "functions": {
    "app/**": {
      "includeFiles": "static/**"
    }
  },
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/static/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/static/robots.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}